{
    "name": "moonbaselabs/blade-components",
    "description": "A set of themeable components built with Tailwind CSS and AlpineJS for use with Laravel Blade.",
    "keywords": ["Laravel", "Blade", "UI", "Tailwind", "AlpineJS"],
    "homepage": "https://github.com/moonbaselabs/blade-components",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/moonbaselabs/blade-components/issues",
        "source": "https://github.com/moonbaselabs/blade-components"
    },
    "authors": [
        {
            "name": "Christian Taylor",
            "homepage": "https://imacrayon.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^7.0|^8.0",
        "illuminate/view": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "MoonbaseLabs\\BladeComponents\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MoonbaseLabs\\BladeComponents\\BladeComponentsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
